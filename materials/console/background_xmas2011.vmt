UnlitGeneric
{
	$basetexture "console/background_xmas2011"

	$vertexcolor 1
	$vertexalpha 1
	$ignorez 1
	$nolod 1
	$no_fullbright 1

	%keywords "tf"

	$scale "[0.85 0.85]"
	$translate "[0 0]"

	Proxies
	{
		Sine
		{
			resultVar "$translate[0]"
			sineperiod 90
			sinemin -0.05
			sinemax 0.05
		}

		TextureTransform
		{
			translateVar "$translate"
			scaleVar "$scale"
			resultVar "$basetexturetransform"
		}
	}
}